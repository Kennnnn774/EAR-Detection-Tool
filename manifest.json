{
    "manifest_version": 3,
    "name": "EAR Scanner",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [ "<all_urls>" ]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}